{
	"name": "todo-client",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.5.0",
		"@testing-library/user-event": "^7.2.1",
		"axios": "^0.19.2",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-input-color": "^3.0.0",
		"react-router-dom": "^5.2.0",
		"react-scripts": "3.4.1"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"compile-sass": "node-sass ./src/sass/main.scss -o ./src/style",
		"watch-sass": "node-sass ./src/sass/main.scss ./src/style/main.css -w",
		"concat-css": "concat -o ./style/main.concat.css ./style/main.comp.css",
		"prefix-css": "postcss --use autoprefixer -b 'last 10 versions' ./css/style/main.concat.css -o ./css/style.prefix.css",
		"compress-css": "node-sass ./style/main.prefix.css ./style/main.css --output-style compressed"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"autoprefixer": "^9.8.0",
		"concat": "^1.0.3",
		"node-sass": "^4.14.1",
		"postcss-cli": "^7.1.0"
	}
}
